{
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "jsx": "react-native",
    "types": [
      "react-native"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base",
  "include": [
    "global.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
